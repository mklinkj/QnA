<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
  <Appenders>
    <Console name="STDOUT" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
    </Console>
  </Appenders>

  <Loggers>
    <Logger name="org.mklinkj.qna" level="debug"/>

    <Root level="debug">
      <Appender-Ref ref="STDOUT"/>
    </Root>
  </Loggers>
</Configuration>
